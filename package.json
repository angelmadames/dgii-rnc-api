{
  "name": "dgii-rnc-api",
  "version": "0.0.1",
  "description": "DGII RNC API",
  "author": "Angel M. Adames <angelmadames@gmail.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "production": "node dist/main",
    "format:ci": "biome format src/",
    "format": "biome format src/ --write",
    "lint": "biome lint src/ --apply",
    "lint:ci": "biome lint src/",
    "check:ci": "biome check src/",
    "check": "biome check src/ --apply",
    "migration:create": "typeorm migration:create",
    "migration:run": "typeorm migration:run -d ./dist/database/config.js",
    "migration:revert": "typeorm migration:revert",
    "download-rnc-file": "ts-node ./src/cli.ts download-rnc-file",
    "process-rnc-file": "ts-node ./src/cli.ts process-rnc-file"
  },
  "dependencies": {
    "@inquirer/prompts": "^4.0.0",
    "@nestjs/bull": "^10.1.0",
    "@nestjs/common": "^10.3.2",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.2",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.3.2",
    "@nestjs/typeorm": "^10.0.2",
    "axios": "^1.6.7",
    "bull": "^4.12.2",
    "chalk": "^5.3.0",
    "dotenv": "^16.4.1",
    "nest-commander": "^3.12.5",
    "pg": "^8.11.3",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
