{
  "name": "dgii-rnc-api",
  "version": "0.0.1",
  "description": "DGII RNC API",
  "author": "Angel M. Adames <angelmadames@gmail.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "production": "node dist/main",
    "format": "biome format src/ --write",
    "lint": "biome lint src/",
    "check": "biome check src/ --apply",
    "migration:create": "typeorm migration:create",
    "migration:run": "typeorm migration:run -d ./dist/database/config.js",
    "migration:revert": "typeorm migration:revert",
    "download-rnc-file": "ts-node ./src/cli.ts download-rnc-file",
    "process-rnc-file": "ts-node ./src/cli.ts process-rnc-file"
  },
  "dependencies": {
    "@inquirer/prompts": "^3.2.0",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "axios": "^1.5.0",
    "bull": "^4.12.0",
    "chalk": "^5.3.0",
    "dotenv": "^16.3.1",
    "nest-commander": "^3.12.5",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.1",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/chai": "^4.3.8",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "chai": "^4.3.10",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
